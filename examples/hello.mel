class Animal {
    constructor(n) {
        name = n
    }
    
    speak() {
        return name + " makes a sound"
    }
}

class Dog extends Animal {
    constructor(n, b) {
        breed = b
    }
    
    speak() {
        return name + " barks!"
    }
}

dog = Dog("Rex", "Labrador")
print(dog.speak())    // Rex barks!
print(dog.name)       // Rex
print(dog.breed)      // Labrador